        "Interval": 5,
        "NumberOfRepetitions": 5,
        "CompanyTypes": [ "Small", "Medium", "Large" ]
      },
      {
        "Market": "Norway",
        "Interval": 5,
        "NumberOfRepetitions": 4,
        "CompanyTypes": [ "Small", "Medium", "Large" ]
      },
      {
        "Market": "Sweden",
        "Interval": 5,
        "NumberOfRepetitions": 4,
        "CompanyTypes": [ "Small", "Medium" ]
      },
      {
        "Market": "Finland",
        "Interval": 5,
        "NumberOfRepetitions": 5,
        "CompanyTypes": [ "Large" ]
      }
    ]
  }
}